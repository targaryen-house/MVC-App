@model IEnumerable<MVCswitchback.Models.UserInfo>
@{
    ViewData["Title"] = "Index";
    Layout = "_Layout";
}

<h4>All Users</h4>

<p>
    <a asp-action="Create">Add User</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th style="width: 25%;">
                User ID
            </th>
            <th style="width: 25%;">
                User Name
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td style="width: 25%;">
                    @item.ID
                </td>
                <td style="width: 25%;">
                    @item.UserName
                </td>
            </tr>
        }
    </tbody>
</table>
